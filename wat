  513  clear
  514  arr_variable=("components" "cypress" "hooks" "lib" "pages" "store")
  515  ## now loop through the above array
  516  for i in "${arr_variable[@]}"; do     echo "$i: $(find $i -mindepth 1 -type f -name "*.ts" -printf x | wc -c) .ts and \
  517  $(find $i -mindepth 1 -type f -name "*.tsx" -printf x | wc -c) .tsx out of \
  518  $(find $i -mindepth 1 -type f -name "*.js" -printf x | wc -c) .js"; done
  519  clear
  520  arr_variable=("components" "cypress" "hooks" "lib" "pages" "store")
  521  ## now loop through the above array
  522  for i in "${arr_variable[@]}"; do     echo "$i: $(find $i -mindepth 1 -type f -name "*.ts" -printf x | wc -c) .ts and \
  523  $(find $i -mindepth 1 -type f -name "*.tsx" -printf x | wc -c) .tsx compared to \
  524  $(find $i -mindepth 1 -type f -name "*.js" -printf x | wc -c) .js"; done
